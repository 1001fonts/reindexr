{
    "name": "basster/reindexr",
    "description": "A CLI tool to repartition elasticsearch indices",
    "type": "project",
    "require": {
        "symfony/console": "^5.1",
        "php-di/php-di": "^6.2",
        "php": ">=7.4",
        "ruflin/elastica": "^6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ole Rößner",
            "email": "ole@roessner.it"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Basster\\Reindexr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Basster\\Reindexr\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
